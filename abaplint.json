{
  "global": {
    "files": "/src/**/*.*", 
    "exclude": ["/src/zabap_translations_test/*"],
    "noIssues": [],
    "skipGeneratedBOPFInterfaces": false,
    "skipGeneratedFunctionGroups": false,
    "skipGeneratedGatewayClasses": false,
    "skipGeneratedPersistentClasses": false,
    "skipGeneratedProxyClasses": false,
    "skipGeneratedProxyInterfaces": false,
    "useApackDependencies": false,
    "skipIncludesWithoutMain": false
  },
  "dependencies": [
    {
      "url": "https://github.com/abaplint/deps",
      "folder": "/deps",
      "files": "/src/**/*.*"
    }
  ],
  "syntax": {
    "version": "v750",
    "errorNamespace": "^(Z|Y|LCL_|TY_|LIF_)",
    "globalConstants": [],
    "globalMacros": []
  },
  "rules": {
    "7bit_ascii": {
      "exclude": [],
      "severity": "Error"
    },
    "allowed_object_naming": {
      "exclude": [],
      "severity": "Error"
    },
    "ambiguous_statement": {
      "exclude": [],
      "severity": "Error"
    },
    "avoid_use": {
      "exclude": [],
      "severity": "Error",
      "skipQuickFix": false,
      "define": false,
      "statics": true,
      "defaultKey": true,
      "break": true,
      "testSeams": true,
      "describeLines": true,
      "exportToMemory": false,
      "exportToDatabase": false
    },
    "begin_end_names": {
      "exclude": [],
      "severity": "Error"
    },
    "begin_single_include": {
      "exclude": [],
      "severity": "Error"
    },
    "call_transaction_authority_check": {
      "exclude": [],
      "severity": "Warning"
    },
    "cds_comment_style": {
      "exclude": [],
      "severity": "Error"
    }, 
    "cds_parser_error": {
      "exclude": [],
      "severity": "Error"
    }, 
    "check_ddic": {
      "exclude": [],
      "severity": "Warning"
    },
    "check_include": {
      "exclude": [],
      "severity": "Error"
    },
"check_syntax": {
  "exclude": [],
  "severity": "Error"
},
    "check_transformation_exists": {
      "exclude": [],
      "severity": "Warning"
    }, 
    "constructor_visibility_public": {
      "exclude": [],
      "severity": "Error"
    }, 
    "dangerous_statement": {
      "exclude": [],
      "severity": "Warning",
      "execSQL": true,
      "kernelCall": true,
      "systemCall": true,
      "insertReport": true,
      "generateDynpro": true,
      "generateReport": true,
      "generateSubroutine": true,
      "deleteReport": true,
      "deleteTextpool": true,
      "deleteDynpro": true,
      "exportDynpro": true,
      "dynamicSQL": true
    },
    "dynpro_checks": {
      "exclude": [],
      "severity": "Error"
    },
    "exit_or_check": {
      "exclude": [],
      "severity": "Error",
      "allowExit": false,
      "allowCheck": false
    },
    "form_tables_obsolete": {
      "exclude": [],
      "severity": "Error"
    },
    "fully_type_constants": {
      "exclude": [],
      "severity": "Error",
      "checkData": true
    },
    "fully_type_itabs": {
      "exclude": [],
      "severity": "Error"
    },
    "function_module_recommendations": {
