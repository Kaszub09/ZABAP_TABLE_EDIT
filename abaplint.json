    "unsecure_fae": {
      "exclude": [],
      "severity": "Warning"
    },
    "use_bool_expression": {
      "exclude": [],
      "severity": "Error"
    },
    "use_class_based_exceptions": {
      "exclude": [],
      "severity": "Error"
    },
    "use_line_exists": {
      "exclude": [],
      "severity": "Error"
    }
  }
