<?xml version="1.0" encoding="utf-8"?>
<abapGit version="v1.0.0" serializer="LCL_OBJECT_INTF" serializer_version="v1.0.0">
 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <VSEOINTERF>
    <CLSNAME>ZIF_ZABAP_TABLE_EDIT</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>Interface to implement to extend Table edit - see examples</DESCRIPT>
    <EXPOSURE>2</EXPOSURE>
    <STATE>1</STATE>
    <UNICODE>X</UNICODE>
   </VSEOINTERF>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>ADDITIONAL_FIELDS</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Allows to extend displayed table - fill fields manually...</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ADDITIONAL_VALIDATION</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>After internal validation. Tables are base, not extended</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>AFTER_COMMAND</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Not called if command was cancelled</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>AFTER_SAVE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>After save. Tables are base, not extended</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>BEFORE_COMMAND</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Can execute custom command and alter default behavior</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>BEFORE_SAVE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>After validation, before save. Base tables, not extended...</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>CHANGE_COMMANDS</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Called after setting up the screen - edit class...</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>GRID_SETUP</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Called once, e.g. modify commands / hook up other events</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>INITIAL_DATA</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Display / table edit will be based on this data.</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ON_DATA_CHANGED</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>CL_GUI_ALV_GRID event - hooked up automatically</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ON_DATA_CHANGED_FINISHED</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>CL_GUI_ALV_GRID event - hooked up automatically</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>REFRESH_GRID</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Called whenever display mode is changed/refreshed, ...</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>REPLACE_INITIAL_DATA_SELECT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Display / table edit will be based on this data.</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>SET_EDIT_MODE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Called whenever edit mode is changed</DESCRIPT>
    </SEOCOMPOTX>
   </DESCRIPTIONS>
   <DESCRIPTIONS_SUB>
    <SEOSUBCOTX>
     <CMPNAME>ADDITIONAL_FIELDS</CMPNAME>
     <SCONAME>ADDITIONAL_FIELDS</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Names must be unique and different from &apos;CHANGE_INDICATOR&apos;</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>ADDITIONAL_VALIDATION</CMPNAME>
     <SCONAME>RESULT</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Of type c_validation in this interface</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>AFTER_COMMAND</CMPNAME>
     <SCONAME>COMMAND</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Can be modified to alter called command</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>BEFORE_COMMAND</CMPNAME>
     <SCONAME>CANCEL_COMMAND</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Doesn&apos;t execute command</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>BEFORE_COMMAND</CMPNAME>
     <SCONAME>COMMAND</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Can be modified to alter called command</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>CHANGE_COMMANDS</CMPNAME>
     <SCONAME>IN_EDIT_MODE</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>If called in edit mode</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>INITIAL_DATA</CMPNAME>
     <SCONAME>INITIAL_DATA</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Ref to table of given type with all data from table...</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>REFRESH_GRID</CMPNAME>
     <SCONAME>INITIAL_DATA</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Ref to table of given type with all data from table...</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>REFRESH_GRID</CMPNAME>
     <SCONAME>IN_EDIT_MODE</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>If called in edit mode</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>REPLACE_INITIAL_DATA_SELECT</CMPNAME>
     <SCONAME>INITIAL_DATA</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Ref to table of given type with all data from table...</DESCRIPT>
    </SEOSUBCOTX>
   </DESCRIPTIONS_SUB>
  </asx:values>
 </asx:abap>
</abapGit>
