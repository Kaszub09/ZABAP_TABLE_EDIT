<?xml version="1.0" encoding="utf-8"?>
<abapGit version="v1.0.0" serializer="LCL_OBJECT_INTF" serializer_version="v1.0.0">
 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <VSEOINTERF>
    <CLSNAME>ZIF_ZABAP_TABLE_EDIT_DATA</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>Data manipulation extension</DESCRIPT>
    <EXPOSURE>2</EXPOSURE>
    <STATE>1</STATE>
    <UNICODE>X</UNICODE>
   </VSEOINTERF>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>ADDITIONAL_FIELDS</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Allows to extend displayed table - fill fields manually...</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ADDITIONAL_VALIDATION</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>After internal validation. Tables are base, not extended</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>AFTER_SAVE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>After save and COMMIT. Tables are base, not extended</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>BEFORE_SAVE</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>After validation, before save. Base tables, not extended...</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>CHANGE_DISPLAY_TEXT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Called before display, after grid refresh</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>DEFAULT_SELECT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Can be used to disable execution of default select</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>INITIAL_DATA</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Display / table edit will be based on this data.</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ON_DATA_CHANGED</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>CL_GUI_ALV_GRID event - hooked up automatically</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>ON_DATA_CHANGED_FINISHED</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>CL_GUI_ALV_GRID event - hooked up automatically</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>REFRESH_GRID</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Called whenever display mode is changed/refreshed, ...</DESCRIPT>
    </SEOCOMPOTX>
   </DESCRIPTIONS>
   <DESCRIPTIONS_SUB>
    <SEOSUBCOTX>
     <CMPNAME>ADDITIONAL_FIELDS</CMPNAME>
     <SCONAME>ADDITIONAL_FIELDS</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Names must be unique and different from &apos;CHANGE_INDICATOR&apos;</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>ADDITIONAL_VALIDATION</CMPNAME>
     <SCONAME>RESULT</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Of type c_validation in this interface</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>DEFAULT_SELECT</CMPNAME>
     <SCONAME>EXECUTE</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>If set to abap_false default select will not be executed</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>INITIAL_DATA</CMPNAME>
     <SCONAME>INITIAL_DATA</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Ref to table with all/none data from table...</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>REFRESH_GRID</CMPNAME>
     <SCONAME>INITIAL_DATA</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Ref to table of given type with non-modified data</DESCRIPT>
    </SEOSUBCOTX>
    <SEOSUBCOTX>
     <CMPNAME>REFRESH_GRID</CMPNAME>
     <SCONAME>IN_EDIT_MODE</SCONAME>
     <LANGU>E</LANGU>
     <DESCRIPT>If called in edit mode</DESCRIPT>
    </SEOSUBCOTX>
   </DESCRIPTIONS_SUB>
  </asx:values>
 </asx:abap>
</abapGit>
